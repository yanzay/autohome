<form method="POST">
  <div id="schedulerItems">
  {{range .}}
    <div class="form-group row">
      <div class="col-sm-3">
        <input type="text" class="form-control" name="cronStrings[]" value="{{.CronString}}">
      </div>
      <div class="col-sm-3">
        <input type="text" class="form-control" name="funcNames[]" value="{{.FuncName}}">
      </div>
      <a href="javascript:void(0);" class="btn btn-danger deleteItem">Delete</a>
    </div>
  {{end}}
  </div>
  <div class="form-group">
    <a href="javascript:void(0);" id="addItem" class="btn btn-success">Add</a>
    <input type="submit" class="btn btn-primary">
  </div>
</form>
